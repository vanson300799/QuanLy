@model WebModels.UploadFile

@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutEmpty.cshtml";

}

@using (Html.BeginForm("Delete", "UploadFile", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal fix" }))
{
    <div class="alert alert-info">
        @Html.Raw(Resources.Common.DeleteSureItem):  <strong>@Html.DisplayFor(m => m.Title)</strong>  
    </div>

    <div class="validation-group">
        @Html.ValidationSummary(true)
    </div>

    <div class="form-actions">
    <button class="btn btn-primary" type="submit">@Resources.Common.Delete</button>
</div>
    @Html.HiddenFor(m => m.Title)
    @Html.AntiForgeryToken()
}
<script type="text/javascript">
    function delete_success() {
        top.winclose();
        top.$("#grid").data("kendoGrid").dataSource.read();
    }
</script>
