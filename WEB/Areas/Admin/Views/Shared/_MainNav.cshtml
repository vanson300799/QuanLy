<ul id="mainnav" class='main-nav'> </ul>
<script>
    if ($.cookie('mainnav')) $('#mainnav').html($.cookie('mainnav'));
    $(document).ready(function () { 
        $.get("/admin/adminsite/gethtmlsiteonrole/", function (data) { 
            $('#mainnav').html(data);
            var date = new Date();
            date.setTime(date.getTime() + (10 * 60 * 1000));
            $.cookie('mainnav', data, { expires: date });
        }); 
    });
</script>

