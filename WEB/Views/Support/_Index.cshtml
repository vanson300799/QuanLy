@model IEnumerable<SupportOnline>
           
@if (Model.Count() > 0) { 
<h3>Hỗ trợ</h3>
<div class="content-box">
    @foreach (var item in Model)
    {
        <div class="support">
            <span>@item.Phone</span>
            <br />
            <p class="skype"><a href="ymsgr:sendim?@item.Skype&amp;m=Hello."> Skype : @item.Title</a></p>
            <p class="yahoo"><a href="ymsgr:sendim?@item.Yahoo&amp;m=Hello."> Yahoo : @item.Title</a></p>
        </div>   
    }
</div>
}